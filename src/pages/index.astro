---
import Layout from "@layouts/layout.astro";
import Footer from "@layouts/footer.astro";
import Banner from "@page/home/banner.astro";
import Nosotros from "@page/home/nosotros.astro";
import Cursos from "@page/home/cursos.astro";
import Preguntas from "@page/home/preguntas.astro";
import Unete from "@page/home/unete.astro";
import CursosBlog from "@page/home/cursos-blog.astro";
---

<Layout>
  <main class="h-screen overflow-y-scroll snap-y snap-mandatory">
    <Banner />
    <Nosotros />
    <Cursos />
    <CursosBlog />
    <Unete />
    <Preguntas />
    <Footer />
  </main>
</Layout>
