---
import "../styles/global.css";
import Layout from "../layouts/layout.astro";
import Navbar from "../layouts/navbar.astro";
import Footer from "../layouts/footer.astro";
import Banner from "../page/banner.astro";
import Nosotros from "../page/nosotros.astro";
import Cursos from "../page/cursos.astro";
import Preguntas from "../page/preguntas.astro";
import Unete from "../page/unete.astro";
---

<Layout>
  <Navbar />
  <main class="h-screen overflow-y-scroll snap-y snap-mandatory">
    <Banner />
    <Nosotros />
    <Cursos />
    <Unete />
    <Preguntas />
    <Footer />
  </main>
</Layout>
